{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "module"
	},
	"globals": {
		"module": false
	},
	"rules": {
		"indent": [
			"warn",
			"tabs"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"off",
			"never"
		],
		"no-console": 0
	}
}